{
  "main": "./dist/src/index.js",
  "scripts": {
    "build:models": "npm run prisma-generate && npm run schemas-generate",
    "build": "npm run build:models && tsc && tsc-alias",
    "dev": "nodemon",
    "dev:docker": "nodemon --config ./nodemon.json -L -V",
    "start": "node ./dist/src/index.js",
    "start:dev": "node --trace-warnings ./dist/src/index.js",
    "watch:models": "nodemon -w src/**/schemas.ts *.prisma -e js,prisma -x \"npm run build:models\"",
    "schemas-generate": "ts-node ./scripts/gen-schemas",
    "prisma-generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma-seed": "tsc && tsc-alias && node ./dist/src/prisma/seed.js",
    "prisma-studio": "npx prisma studio --schema=./src/prisma/schema.prisma -p 5353 -n localhost"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.1",
    "@stylistic/eslint-plugin-plus": "^1.5.1",
    "@stylistic/eslint-plugin-ts": "^1.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.3",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/pino-pretty": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "fast-json-stringify": "^5.9.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "prisma": "^5.7.0",
    "ts-json-schema-generator": "^1.4.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.62.0"
  },
  "dependencies": {
    "@fastify/auth": "^4.4.0",
    "@fastify/bearer-auth": "^9.1.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/oauth2": "^7.8.0",
    "@fastify/static": "^6.12.0",
    "@prisma/client": "^5.7.1",
    "@types/source-map-support": "^0.5.10",
    "@uploadcare/upload-client": "^6.10.0",
    "ajv": "^8.12.0",
    "app-root-path": "^3.1.0",
    "bcrypt": "^5.1.1",
    "config": "^3.3.9",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-healthcheck": "^4.4.0",
    "fastify-plugin": "^4.5.1",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21"
  }
}
